{
    "name": "antoinedly/router",
    "description": "A simple Router",
    "type": "library",
    "homepage": "https://github.com/AntoineDly/Router",
    "license": "MIT",
    "authors": [
        {
            "name": "Antoine Delaunay",
            "email": "antoine.delaunay333@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "psr/container": "^2.0",
        "psr/http-factory": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-message": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "AntoineDly\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AntoineDly\\Router\\": "tests/"
        }
    },
    "provide": {
        "psr/container-implementation": "2.0.2"
    }
}
